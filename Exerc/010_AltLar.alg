algoritmo "LaAl"
var
   Alt, Lar, Are, Tin: real
inicio
   escreva("Digite a altura da parede: ")
   leia(Alt)
   escreva("Digite a largura da parede: ")
   leia(Lar)
   Are <- Alt * Lar
   Tin <- Are/2
   escreval("A área a ser pintada é de: ", Are)
   escreva("A quantidade de tinta nescessária para pintar, é de: ", Tin)

fimalgoritmo
